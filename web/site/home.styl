  p
    color #666
    text-align left
    strong
      font-family "Lucida Sans Unicode","Lucida Grande",sans-serif !important
      color #2c3e51

.testimonial
  padding 0px 20px
  max-width 80%
  margin 0 auto
  padding 40px 0
  clear both
